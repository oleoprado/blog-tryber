AAC;QACpC,CAAC;KAAA;IAEK,QAAQ,CAAC,EAAU;;YACvB,MAAM,IAAI,CAAC,kBAAkB,CAAC,EAAE,CAAC,CAAC;YAElC,MAAM,IAAI,GAAG,MAAM,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,EAAE,CAAC,CAAC;YAC3C,OAAO,IAAY,CAAC;QACtB,CAAC;KAAA;IAEK,MAAM,CAAC,EAAU,EAAE,GAAU;;YACjC,MAAM,IAAI,CAAC,kBAAkB,CAAC,EAAE,CAAC,CAAC;YAElC,MAAM,IAAI,CAAC,KAAK,CAAC,MAAM,mBAAM,GAAG,GAAI,EAAE,KAAK,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC,CAAC;YACvD,MAAM,WAAW,GAAG,MAAM,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,EAAE,CAAC,CAAC;YAClD,OAAO,WAAmB,CAAC;QAC7B,CAAC;KAAA;IAEK,MAAM,CAAC,EAAU;;YACrB,MAAM,IAAI,CAAC,kBAAkB,CAAC,EAAE,CAAC,CAAC;YAClC,MAAM,IAAI,CAAC,KAAK,CAAC,OAAO,CAAC,EAAE,KAAK,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC,CAAC;QAC9C,CAAC;KAAA;IAEa,kBAAkB,CAAC,EAAU;;YACzC,MAAM,IAAI,GAAG,MAAM,IAAI,CAAC,KAAK,CAAC,QAAQ,CAAC,EAAE,CAAC,CAAC;YAC3C,IAAI,CAAC,IAAI;gBAAE,MAAM,IAAI,KAAK,CAAC,gBAAgB,EAAE,YAAY,CAAC,CAAC;QAC7D,CAAC;KAAA;CACF;AAnCD,8BAmCC","sourcesContent":["import { ModelStatic } from \"sequelize\";\nimport Post from \"../../database/models/Post\";\nimport IPost from \"../interfaces/IPost\";\nimport IServicePost from \"../interfaces/IServicePost\";\n\nexport default class PostService implements IServicePost {\n  protected model: ModelStatic<Post> = Post // modelStatic(type do sequelize) é a represetação das tabelas do db. Aqui esta inicializando a model.\n  \n  async create(dto: IPost): Promise<Post> {\n    return await this.model.create({ ...dto });\n  }\n\n  async readAll(): Promise<Post[]> {\n    return await this.model.findAll();\n  }\n\n  async readById(id: number): Promise<Post> {\n    await this._verifyIfPostExist(id);\n   \n    const post = await this.model.findByPk(id);\n    return post as Post;\n  }\n\n  async update(id: number, dto: IPost): Promise<Post> {\n    await this._verifyIfPostExist(id);\n\n    await this.model.update({ ...dto }, { where: { id } });\n    const updatedPost = await this.model.findByPk(id);\n    return updatedPost as Post;\n  }\n\n  async delete(id: number): Promise<void> {\n    await this._verifyIfPostExist(id);\n    await this.model.destroy({ where: { id } });\n  }\n\n  private async _verifyIfPostExist(id: number): Promise<void> {    \n    const post = await this.model.findByPk(id);\n    if (!post) throw new Error(`Post with id ${id} not found`);\n  }\n}\n\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"256609bb95feb5d595026a2c84bd93ffa1310f03"}}